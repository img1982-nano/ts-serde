  "homepage": "https://github.com/jill64/ts-serde#readme",
  "files": [
    "dist"
  ],
  "exports": {
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "default": "./dist/utils/index.js"
    },
    "./object": {
      "types": "./dist/object/index.d.ts",
      "default": "./dist/object/index.js"
    },
    "./primitive": {
      "types": "./dist/primitive/index.d.ts",
      "default": "./dist/primitive/index.js"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "default": "./dist/types/index.js"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jill64/ts-serde.git",
    "image": "https://opengraph.githubassets.com/90c79ba6acddb92d14c7a5814680462889ee9da4263cd89b03ee82bf6d42faea/jill64/ts-serde"
  },
  "keywords": [
    "deserialize",
    "serde",
    "serialize",
    "type-safe"
  ],
  "scripts": {
    "build": "tsc && npx publint",
    "prepack": "npm run build",
    "lint": "npx eslint . && npx depcheck",
    "format": "npx prettier -w .",
    "test": "vitest"
  },
  "prettier": "@jill64/prettier-config",
  "devDependencies": {
    "@jill64/eslint-config-ts": "1.1.46",
    "@jill64/prettier-config": "1.0.0",
    "typescript": "5.6.3",
    "vitest": "2.1.3"
  },
  "dependencies": {
    "devalue": "5.1.1"
  }
}
