  "homepage": "https://github.com/jill64/ts-serde#readme",
  "files": [
    "dist"
  ],
  "exports": {
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "default": "./dist/utils/index.js"
    },
    "./object": {
      "types": "./dist/object/index.d.ts",
      "default": "./dist/object/index.js"
    },
    "./primitive": {
      "types": "./dist/primitive/index.d.ts",
      "default": "./dist/primitive/index.js"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "default": "./dist/types/index.js"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jill64/ts-serde.git",
    "image": "https://opengraph.githubassets.com/26e292e2d1aea999936a6056c0d146694510b6c509ef1b69affc997377a8bb83/jill64/ts-serde"
  },
  "keywords": [
    "deserialize",
    "serde",
    "serialize",
    "type-safe"
  ],
  "scripts": {
    "build": "tsc && npx publint",
    "prepack": "npm run build",
    "lint": "npx eslint . && npx depcheck",
    "format": "npx prettier -w .",
    "test": "vitest"
  },
  "prettier": "@jill64/prettier-config",
  "devDependencies": {
    "@jill64/eslint-config-ts": "2.0.3",
    "@jill64/prettier-config": "1.0.0",
    "typescript": "5.7.2",
    "vitest": "2.1.8"
  },
  "dependencies": {
    "devalue": "5.1.1"
  }
}
