{
  "name": "ts-serde",
  "description": "ðŸŽ¶ Typed Serialization and Deserialization",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "author": "jill64 <intents.turrets0h@icloud.com> (https://github.com/jill64)",
  "bugs": "https://github.com/jill64/ts-serde/issues",
  "homepage": "https://github.com/jill64/ts-serde#readme",
  "files": [
    "dist"
  ],
  "exports": {
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "default": "./dist/utils/index.js"
    },
    "./object": {
      "types": "./dist/object/index.d.ts",
      "default": "./dist/object/index.js"
    },
    "./primitive": {
      "types": "./dist/primitive/index.d.ts",
      "default": "./dist/primitive/index.js"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "default": "./dist/types/index.js"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jill64/ts-serde.git"
  },
  "keywords": [
    "deserialize",
    "serde",
    "serialize",
    "type-safe"
  ],
  "scripts": {
    "build": "tsc && npx publint",
    "prepack": "npm run build",
    "lint": "npx eslint . && npx depcheck",
    "format": "npx prettier -w .",
    "test": "vitest"
  },
  "prettier": "@jill64/prettier-config",
  "devDependencies": {
    "@jill64/eslint-config-ts": "1.0.1",
    "@jill64/prettier-config": "1.0.0",
    "typescript": "5.3.3",
    "vitest": "1.0.4"
  },
  "dependencies": {
    "devalue": "4.3.2"
  }
}
